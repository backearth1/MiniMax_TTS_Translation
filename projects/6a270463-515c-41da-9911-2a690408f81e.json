    },
    {
      "id": "e56997d7-1e4a-4ba9-b91b-55d1520e1d79",
      "index": 41,
      "start_time": "00:01:56,780",
      "end_time": "00:01:58,000",
      "speaker": "SPEAKER_00",
      "text": "是不是用老方法結帳？",
      "emotion": "auto",
      "speed": 1.0,
      "has_audio": false,
      "audio_duration": 0,
      "audio_url": null,
      "trace_id": null,
